{
	"title": "Settings", "type": "section", "items": [
		{ "title": "Theme", "type": "section", "items": [
			{ "type": "special", "subtype": "theming-header" },
			{ "title": "Use page color for browser", "type": "toggle", "id": "use_layout_color_from_page" }
		] },

		{ "title": "Customization", "type": "section", "items": [
			{
				"title": "Collapse bars",
				"description": "While scrolling navigation bars will collapse and expand.",
				"id": "collapse_bars", "type": "toggle"
			}, {
				"title": "Left actions",
				"description": "If topbar is here, they'll appear in it, or else in the bottombar.",
				"type": "action",
				"id": "customize_left_actions"
			}, {
				"title": "Right actions",
				"description": "If topbar is here, they'll appear in it, or else in the bottombar.",
				"type": "action",
				"id": "customize_right_actions"
			}, {
				"title": "Side actions",
				"description": "If sidebar is here, they'll appear in it, or else in the popup menu.",
				"type": "action",
				"id": "customize_side_actions"
			}, {
				"title": "Popup actions",
				"description": "They'll appear in the popup menu",
				"type": "action",
				"id": "customize_menu_actions"
			}
		] },

		{ "title": "Search", "type": "section", "items": [
			{ "title": "Search engine", "type": "ratio", "id": "search_engine", "items": [
				{ "title": "Google", "value": "google" },
				{ "title": "Bing", "value": "bing" },
				{ "title": "DuckDuckGo", "value": "duckduckgo" },
				{ "title": "Yandex", "value": "yandex" },
				{ "title": "Yahoo", "value": "yahoo" }
			] }, {
				"title": "Search engine auto completion",
				"description": "Get quick search query suggestions while typing from a selected search engine.",
				"id": "search_engine_autocompletion",
				"type": "toggle"
			}, {
				"title": "Search history auto completion",
				"description": "Get search suggestions from history",
				"id": "search_history_autocompletion",
				"type": "toggle"
			}
		] },

		{ "title": "Security", "type": "section" },

		{ "title": "Saved", "type": "section", "values": [
			{ "title": "Clear cache", "type": "action", "id": "clear_cache", "description": "$APP_SIZE_CACHE" },
			{ "title": "Clear history", "type": "action", "id": "clear_history", "description": "$APP_SIZE_HISTORY" },
			{ "title": "Clear forms data", "type": "action", "id": "clear_forms", "description": "$APP_SIZE_FORMS" },
			{ "title": "Clear cookies", "type": "action", "id": "clear_cookies", "description": "$APP_SIZE_COOKIES" }
		] },

		{ "title": "About", "type": "section", "items": [
			{ "title": "Made by", "description": "MrBoomDev in $APP_BUILD_YEAR" },
			{ "title": "Version", "description": "$APP_BUILD_VERSION" }
		] }
	]
}